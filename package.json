{
   "private": true,
   "scripts": {
      "dev": "mix",
      "watch": "mix watch",
      "watch-poll": "mix watch -- --watch-options-poll=1000",
      "hot": "mix watch --hot",
      "build": "mix --production",
      "clean": "rm -r public/dist",
      "check": "prettier --check \"./**/*.{js, json, scss}\"",
      "format": "prettier --write \"./**/*.{js, json, scss}\"",
