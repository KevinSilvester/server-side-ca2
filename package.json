      "browser-sync": "^2.27.8",
      "browser-sync-webpack-plugin": "^2.3.0",
      "css-loader": "^6.7.1",
      "cssnano": "^5.1.4",
      "jsonfile": "^6.1.0",
      "laravel-mix": "^6.0.43",
      "lodash": "^4.17.21",
      "mini-css-extract-plugin": "^2.6.0",
      "postcss": "^8.4.8",
      "postcss-loader": "^6.2.1",
      "prettier": "^2.5.1",
      "resolve-url-loader": "^5.0.0",
      "sass": "^1.49.9",
      "sass-loader": "^12.6.0",
      "style-loader": "^3.3.1",
      "swc-loader": "^0.1.15",
      "tailwindcss": "^3.0.23",
